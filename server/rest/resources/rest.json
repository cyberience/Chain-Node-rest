{
  "network": {
    "name": "mijinTest",
    "description": "catapult development network"
  },

  "port": 3000,
  "crossDomainHttpMethods": ["GET", "POST", "PUT", "OPTIONS"],
  "clientPrivateKey": "7AF0508F3FA061FCCB5BE5C0BA235CE9BA6039AD744703A1A7A3CF9AAF198DD3",
  "extensions": ["aggregate", "lock", "multisig", "namespace", "transfer"],

  "db": {
    "url": "mongodb://localhost:27017/",
    "name": "catapult",
    "pageSizeMin": 10,
    "pageSizeMax": 100,
    "pageSizeStep": 25,
    "maxConnectionAttempts": 5,
    "baseRetryDelay": 500
  },

  "apiNode": {
    "host": "127.0.0.1",
    "port": 7900,
    "publicKey": "75B513BEDB2B706A132E99A3F9B7F42C5CA3B2B531035EA7012F924AD69C639B",
    "timeout": 1000
  },

  "websocket": {
    "mq": {
      "host": "127.0.0.1",
      "port": 7902,
      "monitorInterval": 500,
      "connectTimeout": 10000,
      "monitorLoggingThrottle": 60000
    },
    "allowOptionalAddress": true
  },

  "logging": {
    "console": {
      "colorize": true,
      "level": "verbose",
      "handleExceptions": true,
      "prettyPrint": true,
      "timestamp": true
    },
    "file": {
      "level": "verbose",
      "handleExceptions": true,
      "prettyPrint": true,
      "timestamp": true,
      "filename": "catapult-rest.log",
      "maxsize": 20971520,
      "maxFiles": 100
    }
  }
}
